<div class="mt-4">
    <div class="row"></div>
        {{#each products}}
        <div class="col-sm-6 col-lg-4">
            <div class="card">
            <img class="card-img-top" src="{{this.image}}" alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>
                    <p class="card-text">{{this.price}}</p>
                    <a href="#" class="btn btn-primary">Go somewhere</a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>
