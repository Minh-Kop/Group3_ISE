<div class="mt-4">
    <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-indicators">
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
        </div>
        <div class="carousel-inner">
            <div class="carousel-item active">
            <img src="https://images.contentstack.io/v3/assets/blt818b0c67cf450811/bltb6c010ff80c5db1c/63bc3f597f7baa7b8ce59d76/Primary_Desktop_(6).jpg" class="d-block w-100" alt="..." width="75%">
            </div>
            <div class="carousel-item">
            <img src="https://images.contentstack.io/v3/assets/blt818b0c67cf450811/blt94e8854e907d6dee/63bc905818cb6062ad644ab8/Best_of_Supreme_2022Primary_Desktop.jpg" class="d-block w-100" alt="..." width="75%">
            </div>
            <div class="carousel-item">
            <img src="https://images.contentstack.io/v3/assets/blt818b0c67cf450811/blte955cb0fab28e403/63b8a3d2c6fa263ecb18a46f/JJJJound_Bape_Sta_assetsPrimary_Desktop.jpg" class="d-block w-100" alt="..." width="75%">
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>

    <div class="brand_section">
        <div class="brand_header" style="font-family: 'Times New Roman', Times, serif; font-size:x-large;; margin-top: 20px; text-decoration: underline; font-weight: bold;">
            <p>Popular Brands</p>
        </div>
        <div class="row">
            <div class="col" style="align-items: center">
                <a href="/jordan"  style="text-decoration: none; font-size:larger">
                    <img src="https://www.pngmart.com/files/22/Jordans-Logo-PNG-Photos.png" style="background-color: white;">
                    <p>Jordan</p>
                </a>
            </div>
            <div class="col" style="align-items: center">
                <a href="/adidas" style="text-decoration: none; font-size:larger">
                    <img src="https://i.pinimg.com/originals/39/a4/fe/39a4fe04374d0139ac5d78a6d24c6017.png" style="background-color: white;">
                    <p>Adidas</p>
                </a>
            </div>
            <div class="col" style="align-items: center">
                <a href="/nike" style="text-decoration: none; font-size:larger">
                    <img src="https://www.vectorkhazana.com/assets/images/products/Nike_8.png" style="background-color: white;">
                    <p>Nike</p>
                </a>
            </div>
            <div class="col" style="align-items: center">
                <a href="/puma" style="text-decoration: none; font-size:larger">
                    <img src="https://www.pngmart.com/files/22/Puma-Logo-PNG-Isolated-HD.png" style="background-color: white;">
                    <p>Puma</p>
                </a>
            </div>
        </div>
    </div>
    
    <div class="gender_section">
        <div class="gender_header" style="font-family: 'Times New Roman', Times, serif; font-size:x-large;; margin-top: 20px; text-decoration: underline; font-weight: bold;">
            <p>Gender </p>
        </div>
        <div class="row">
            <div class="col" style="align-items: center">
                <a href="/male"  style="text-decoration: none; font-size:larger">
                    <img src="https://media.istockphoto.com/id/868651322/vector/male-thin-line-vector-icon.jpg?s=612x612&w=0&k=20&c=hKgPJqnd6bR60CQAmRoxcIQyOYds1p-MlwfKp14YGc0=" width="350" height="350" style="background-color: white;">
                    <p>Male</p>
                </a>
            </div>
            <div class="col" style="align-items: center">
                <a href="/female" style="text-decoration: none; font-size:larger">
                    <img src="https://media.istockphoto.com/id/866757230/vector/female-sex-symbol-icon-black-minimalist-icon-isolated-on-white-background.jpg?s=612x612&w=0&k=20&c=WERPo_gYz_c8PJS8glnOy6WGLgLwQQk7xKzPgLnoOlA=" width="350" height="350" style="background-color: white;">
                    <p>Female</p>
                </a>
            </div>
           
        </div>
    </div>


    <div class="category_section">
        <div class="category_header" style="font-family: 'Times New Roman', Times, serif; font-size:x-large;; margin-top: 20px; text-decoration: underline; font-weight: bold;">
            <p>Category </p>
        </div>
        <div class="row">
            <div class="col" style="align-items: center">
                <a href="/sneaker"  style="text-decoration: none; font-size:larger">
                    <img src="https://i.pinimg.com/originals/ae/24/b1/ae24b1a06a8c3e0972a8a9875aa44318.png" width="350" height="350" style="background-color: white;">
                    <p>Sneaker</p>
                </a>
            </div>
            <div class="col" style="align-items: center">
                <a href="/shoe" style="text-decoration: none; font-size:larger">
                    <img src="https://cdn-icons-png.flaticon.com/512/88/88793.png" width="350" height="350" style="background-color: white;">
                    <p>Shoe</p>
                </a>
            </div>
           
        </div>
    </div>

    <div class="row">
        {{#each products}}
        <div class="col-sm-6 col-lg-4">
            <div class="card card-product-item" style="background-color: rgb(60, 121, 245); height: 350px;">
                <a href="/products/{{this.slug}}">
                    <img class="card-img-top" src="{{this.image}}" alt="{{this.name}}">
                </a>
                <div class="card-body">
                    <a href="/products/{{this.slug}}" style="text-decoration: none; text-transform: uppercase; color: crimson;">
                        <h5 class="card-title">{{this.name}}</h5>
                    </a>
                    <p class="card-text" style="color: crimson;">{{this.price}}</p>
                </div>
            </div>
        </div>
        {{/each}} 
    </div>
</div>